	<category>shipping</category>
	<cartversions>
		<item>2.0</item>
	</cartversions>
	<layout></layout>
	<priority>10</priority>
	<settings>
		<item id="free_shipping_total">
			<type>input</type>
			<default_value></default_value>
		</item>
		<item id="free_shipping_location_id">
			<type>selectbox</type>
			<variants>
				<item>0</item>
				<data_source>
					<model_rt>localisation/location</model_rt>
					<method>getLocations</method>
				</data_source>
				<fields>
					<field>location_id</field>
					<field>name</field>
				</fields>
			</variants>
		</item>
	</settings>
	<install>
		<sql></sql>
		<trigger></trigger>
	</install>
	<uninstall>
		<sql></sql>
		<trigger></trigger>
	</uninstall>
</extension>